  mutate(pop_groups=case_when(
    pop_groups==2 ~ 2,
    pop_groups==4 & citizenship=="BDI" ~ 4,
    pop_groups==4 & citizenship!="BDI" ~ 2,
    pop_groups==5 ~ 5)
