    (COMM01==1 & COMM02 >=2 & COMM03==1 & COMM04==1) ~ 1,
    (COMM01==0 | 
       (COMM02 < 2 | COMM02==98) | 
       (COMM03==0 | COMM03==98) |
       (COMM04==0 | COMM04==98)) ~ 0, TRUE ~ NA_real_)
  ) %>%
