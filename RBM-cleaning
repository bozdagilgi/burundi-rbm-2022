####Output tables and weights -- USE hh SIZE as weights for ind level indicators


###HH01 is the household size variable

##Create table for RBM indicators ETH RAS RMS 2022

##Load packages

install.packages('expss')
install.packages("openxlsx")
library(openxlsx)
library(expss)

###No weights are used for ETH RAS dataset

RBM_Burundimain = main %>% 
  cross_cpct(
    cell_vars = list(impact2_2, impact3_3, outcome4_1, outcome4_2, outcome8_2, outcome9_1,
                     outcome9_2, outcome10_2,outcome12_1, outcome12_2, outcome13_1, outcome13_2,
                     outcome13_3, outcome16_1, outcome16_2),
    col_vars = list(total(), pop_groups),
    weight = NULL) %>% 
  set_caption("RBM Indicators from household dataset")


RBM_Burundiind=ind %>%
  cross_cpct(
    cell_vars = list(impact2_3, impact3_2a, impact3_2b, outcome1_3, outcome5_2, outcome10_1,
                     outcome14_1),
    col_vars = list(total(), pop_groups),
    weight = NULL) %>% 
  set_caption("RBM Indicators from individual dataset")


### EXPORT TO EXCEL


write.xlsx(RBM_Burundimain, "main_Burundi.xlsx", colNames = TRUE, rowNames=TRUE)
write.xlsx(RBM_Burundiind, "ind_Burundi.xlsx", colNames = TRUE, rowNames=TRUE)


table(ind$pop_groups)

table(main$outcome13_3, main$pop_groups)

table(ind$impact3_2a, ind$pop_groups)
table(ind$impact3_2a, ind$pop_groups)

table(ind$impact3_2a)
table(ind$impact3_2b)




### EXPORT TO EXCEL
write.xlsx(main,"main_Burundi_data.xlsx", colNames = TRUE, rowNames=TRUE) 
write.xlsx(ind, "ind_Burundi_data.xlsx", colNames = TRUE, rowNames=TRUE)



